{
  "name": "users-addresses-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "migration:create": "yarn typeorm migration:create src/infra/database/migrations/$nameMigration",
    "migration:generate": "yarn ts-node ./node_modules/typeorm/cli.js migration:generate src/infra/database/migrations/$nameMigration -d src/infra/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/infra/database/data-source-migrations.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/infra/database/data-source-migrations.ts"
  }
}
